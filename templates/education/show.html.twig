{% extends 'base.html.twig' %}

{% block title %}Education{% endblock %}

{% block body %}
    <h1>Education</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ education.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ education.name }}</td>
            </tr>
            <tr>
                <th>School</th>
                <td>{{ education.school }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ education.city }}</td>
            </tr>
            <tr>
                <th>BeginDate</th>
                <td>{{ education.beginDate ? education.beginDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndDate</th>
                <td>{{ education.endDate ? education.endDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_education_index') }}">back to list</a>

    <a href="{{ path('app_education_edit', {'id': education.id}) }}">edit</a>

    {{ include('education/_delete_form.html.twig') }}
{% endblock %}
